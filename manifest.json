{
	"manifest_version": 2,
	"name": "Портативный поисковик",
	"version": "1.0.0",
	"content_scripts": [
	  {
		"matches": [
		  "<all_urls>"
		],
		"js": ["jquery-git.min.js", "main.js"]
	  }
	],
	"browser_action": {
      "default_title": "Тестирование разработки расширения",      // optional; shown in tooltip
      "default_popup": "main.html"        // optional
	},
	"content_security_policy": "script-src 'self' https://cse.google.com; object-src 'self'"
}